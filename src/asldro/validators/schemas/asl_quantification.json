{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "QuantificationModel": {
      "type": "string",
      "enum": ["whitepaper"]
    },
    "PostLabelingDelay": {
      "type": "number",
      "minimum": 0
    },
    "LabelingEfficiency": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "BloodBrainPartitionCoefficient": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "T1ArterialBlood": {
      "type": "number",
      "exclusiveMinimum": 0
    },
    "ArterialSpinLabelingType": {
      "type": "string",
      "enum": ["PCASL", "CASL", "PASL"]
    },
    "LabelingDuration": {
      "type": "number",
      "exclusiveMinimum": 0
    },
    "BolusCutOffDelayTime": {
      "type": "number",
      "minimum": 0
    }
  },
  "additionalProperties": false
}
